{
  // The folder where your Squad logs are saved. e.g "C:/servers/squad_server/SquadGame/Saved/Logs"
  logFile: '',
  // FTP configuration for reading logs remotely.
  ftp: {
    host: '',
    port: 21,
    username: '',
    password: '',
    // The interval in milliseconds to fetch new logs. If you come from SquadJS, default is 0ms (that means your FTP server is called as fast as SquadJS/SquadTS can do), this seems unnecessary often.
    fetchInterval: 1000,
    // The maximum temporary file size in bytes. (probably leave default ?)
    maxTempFileSize: 5000000,
  },
  // SFTP configuration for reading logs remotely.
  sftp: {
    host: '',
    port: 22,
    username: '',
    password: '',
    // The interval in milliseconds to fetch new logs. If you come from SquadJS, default is 0ms (that means your FTP server is called as fast as SquadJS/SquadTS can do), this seems unnecessary often.
    fetchInterval: 1000,
    // The maximum temporary file size in bytes. (probably leave default ?)
    maxTempFileSize: 5000000,
  },
  // - "tail" will read from a local log file
  //     - "ftp" will read from a remote log file using the FTP protocol 
  //     - "sftp" will read from a remote log file using the SFTP protocol
  mode: 'tail',
}